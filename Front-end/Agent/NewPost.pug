doctype html
html(lang='en')
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
		meta(name='description', content='')
		meta(name='author', content='')
		title New Post - Dashboard
		// Bootstrap core CSS
		link(href='vendor/bootstrap/css/bootstrap.min.css', rel='stylesheet')
		// Custom fonts for this template
		link(href='vendor/font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')
		// Page level plugin CSS
		link(href='vendor/datatables/dataTables.bootstrap4.css', rel='stylesheet')
		// Custom styles for this template
		link(href='css/sb-admin.css', rel='stylesheet')
	body#page-top.fixed-nav.sticky-footer.bg-dark
		// Navigation
		nav#mainNav.navbar.navbar-expand-lg.navbar-dark.bg-dark.fixed-top
			a.navbar-brand(href='../index.html') Essos
			button.navbar-toggler.navbar-toggler-right(type='button', data-toggle='collapse', data-target='#navbarResponsive', aria-controls='navbarResponsive', aria-expanded='false', aria-label='Toggle navigation')
				span.navbar-toggler-icon
			form.navbar-form.navbar-search(role='search')
				.input-group
					input.form-control(type='text', style='width: 500px;')
					.input-group-btn
						button.btn.btn-search.btn-primary(type='button', onclick='relocate_home()')
							span.glyphicon.glyphicon-search
							span.label-icon Search
						script.
							function relocate_home()
							{
							location.href = "result.html";
							}
			#navbarResponsive.collapse.navbar-collapse
				ul#exampleAccordion.navbar-nav.navbar-sidenav
					li.nav-item(data-toggle='tooltip', data-placement='right', title='Tables')
						a.nav-link(href='Agent.html')
							i.fa.fa-fw.fa-envelope
							span.nav-link-text Messages
					li.nav-item(data-toggle='tooltip', data-placement='right', title='Tables')
						a.nav-link(href='MyPosts.html')
							i.fa.fa-fw.fa-list
							span.nav-link-text My Posts
					li.nav-item.active(data-toggle='tooltip', data-placement='right', title='Tables')
						a.nav-link(href='NewPost.html')
							i.fa.fa-fw.fa-plus
							span.nav-link-text New Post
				ul.navbar-nav.sidenav-toggler
					li.nav-item
						a#sidenavToggler.nav-link.text-center
							i.fa.fa-fw.fa-angle-left.fa-2x
				ul.navbar-nav.ml-auto
					li.nav-item
						a.nav-link(href='../about.html') About us
					li.nav-item
						a.nav-link.active(href='#') Agent
					li.nav-item
						a.nav-link(data-toggle='modal', data-target='#exampleModal') Sign Out
		// .Navigation
		.content-wrapper
			.container-fluid
				// Breadcrumbs
				ol.breadcrumb
					li.breadcrumb-item
						a(href='Agent.html') Agent Dashboard
					li.breadcrumb-item.active New Post
				// Example DataTables Card
				.card.mb-3
					.card-header
						i.fa.fa-plus-square-o
						|  Enter details to post a new property
					.card-body
						// Page Content
						.container
							form
								.form-group
									label(for='inputAddress') Address line 1
									input#inputAddress1.form-control(type='text', placeholder='1234 Main St')
								.form-group
									label(for='inputAddress2') Address line 2
									input#inputAddress2.form-control(type='text', placeholder='Apartment, studio, or floor')
								.form-row
									.form-group.col-md-6
										label(for='inputCity') City
										input#inputCity.form-control(type='text', placeholder='E.g. San Francisco')
									.form-group.col-md-4
										label(for='inputState') State
										input#inputState.form-control(type='text', placeholder='E.g. California')
									.form-group.col-md-2
										label(for='inputZip') Zip
										input#inputZip.form-control(type='text', placeholder='12345')
								br
								.form-group
									label(for='Title') Title
									input#PropertyTitle.form-control(type='title', placeholder='E.g. Apartment in downtown San Francisco')
								.form-group
									label(for='Description')  Description 
									textarea#PropertyDescription.form-control(rows='3', placeholder='Describe your property')
								.form-row
									.form-group.col-md-2
										label(for='Bedrooms') Bedrooms
										select#Bedrooms.form-control
											option 0
											option 1
											option 2
											option 3
											option 4
											option 5
											option 6
											option 7+
									.form-group.col-md-2
										label(for='Bedrooms') Bathrooms
										select#Bathrooms.form-control
											option 1
											option 1.5
											option 2
											option 2.5
											option 3
											option 3.5
											option 4
											option 4.5
											option 5
											option 5+
									.form-group.col-md-2
										label(for='price') Price
										input#inputPrice.form-control(type='text', placeholder='E.g. $540,000')
								// .row
								.form-group
									label(for='FileUpload') Upload photos
									input#File1.form-control-file(type='file')
									input#File2.form-control-file(type='file')
									input#File3.form-control-file(type='file')
									input#File4.form-control-file(type='file')
									input#File5.form-control-file(type='file')
								.form-group
									.form-check
										label.form-check-label
											input.form-check-input(type='checkbox')
											|  I agree to Essos's terms and conditions
								a.btn.btn-warning.btn-lg(data-toggle='modal', data-target='#CongratulationsModal')  Post
							// .form
						// .container
					.card-footer.small.text-muted Updated yesterday at 11:59 PM
			// /.container-fluid
			// /.content-wrapper
			footer.py-5.bg-dark
				.container
					p.m-0.text-center.text-white Copyright © Team 06 SFSU SE Fall 2017
				// /.container
		// .card mb3
		// .container-fluid
		// Scroll to Top Button
		a.scroll-to-top.rounded(href='#page-top')
			i.fa.fa-angle-up.fa-2x
		// Logout Modal
		#exampleModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')
			.modal-dialog(role='document')
				.modal-content
					.modal-header
						h5#exampleModalLabel.modal-title Ready to Leave?
						button.close(type='button', data-dismiss='modal', aria-label='Close')
							span(aria-hidden='true') ×
					.modal-body Select "Logout" below if you are ready to end your current session.
					.modal-footer
						button.btn.btn-secondary(type='button', data-dismiss='modal') Cancel
						a.btn.btn-primary(href='../index.html') Logout
		// Congratulations Modal
		#CongratulationsModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='CongratulationsModalLabel', aria-hidden='true')
			.modal-dialog(role='document')
				.modal-content
					.modal-header
						h5#exampleModalLabel.modal-title Congratulations!
						button.close(type='button', data-dismiss='modal', aria-label='Close')
							span(aria-hidden='true') ×
					.modal-body Your property has been submitted and will soon be listed on Essos.
					.modal-footer
						a.btn.btn-primary(href='Agent.html') Done
		// Bootstrap core JavaScript
		script(src='vendor/jquery/jquery.min.js')
		script(src='vendor/bootstrap/js/bootstrap.bundle.min.js')
		// Core plugin JavaScript
		script(src='vendor/jquery-easing/jquery.easing.min.js')
		// Page level plugin JavaScript
		script(src='vendor/datatables/jquery.dataTables.js')
		script(src='vendor/datatables/dataTables.bootstrap4.js')
		// Custom scripts for all pages
		script(src='js/sb-admin.min.js')
		// Custom scripts for this page
		script(src='js/sb-admin-datatables.min.js')
		// .content-wrapper

